  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "amp": "find public/amp -type f | xargs amphtml-validator"
  },
  "devDependencies": {
    "amphtml-validator": "^1.0.23",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
