    "gatsby-plugin-feed": "^2.0.13",
    "gatsby-plugin-google-analytics": "^2.0.14",
    "gatsby-plugin-html2amp": "^0.4.0",
    "gatsby-plugin-load-script": "^1.0.6",
    "gatsby-plugin-manifest": "^2.0.18",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sharp": "^2.0.21",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-source-graphql": "2.1.29",
    "gatsby-transformer-sharp": "^2.1.14",
    "graphql-tag": "^2.10.1",
    "node-fetch": "^3.1.1",
    "node-sass": "^4.13.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "typeface-roboto": "^0.0.75"
  },
  "keywords": [
    "fireblog",
    "gatsby",
    "amp",
    "pwa"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireblogcms/gatbsy-starter-fireblog"
